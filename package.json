{
  "name": "@roughapp/sdk",
  "version": "2.8.0",
  "packageManager": "pnpm@10.11.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "George Czabania"
  },
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --run --coverage",
    "tidy": "biome check --write --unsafe",
    "tidy:check": "biome check",
    "knip": "knip",
    "check": "tsc --noEmit",
    "build": "tsup",
    "prepublish": "tsup",
    "openapi-ts": "node ./openapi-ts.ts && ${npm_execpath:-pnpm} run tidy"
  },
  "dependencies": {
    "@hey-api/client-fetch": "0.13.1",
    "@stayradiated/error-boundary": "4.3.0",
    "arctic": "3.7.0",
    "tus-js-client": "4.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@hey-api/openapi-ts": "0.86.8",
    "@types/node": "24.9.2",
    "@vitest/coverage-v8": "4.0.5",
    "knip": "5.66.4",
    "tsup": "8.5.0",
    "typescript": "5.9.3",
    "vite": "7.1.12",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.5"
  },
  "engines": {
    "node": ">=22.9.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "knip": {
    "ignore": [
      "src/api/**"
    ],
    "ignoreDependencies": [
      "@hey-api/client-fetch"
    ]
  }
}
