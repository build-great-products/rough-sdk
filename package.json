    "openapi-ts": "node --experimental-strip-types ./openapi-ts.ts && biome format --write"
  },
  "dependencies": {
    "@hey-api/client-fetch": "0.5.7",
    "@stayradiated/error-boundary": "4.3.0",
    "arctic": "3.0.0"
