{
  "name": "@roughapp/sdk",
  "version": "2.0.0",
  "packageManager": "pnpm@9.15.2",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "author": {
    "name": "George Czabania"
  },
  "license": "MIT",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --run --coverage",
    "tidy": "biome check --write --unsafe",
    "tidy:check": "biome check",
    "knip": "knip",
